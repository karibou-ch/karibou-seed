/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": [],
    "typeRoots": [
      "./node_modules/@types",
      "./src/types"
    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
